  <div class="cms-file-management-main addon-view">
    <div class="list-head">
      <div class="list-head-search">
        <div class="cms-btn-group">
          <button type="button" class="btn btn-state-active" style="padding: 2px 6px; height: 36px; line-height: normal;"><i class="material-icons md-24">view_module</i> グリッド</button>
          <button type="button" class="btn" style="padding: 2px 6px; height: 36px; line-height: normal;"><i class="material-icons md-24">list</i> リスト</button>
          <button type="button" class="btn" style="padding: 2px 6px; height: 36px; line-height: normal;"><i class="material-icons md-24">format_align_justify</i> 詳細リスト</button>
        </div>
      </div>
    </div>

    <dl class="see">
      <dd class="wide">
        <div id="selected-files">
          <% @items.each do |file| %>
            <div class="file-view">
              <%= link_to({ action: :show, id: file.id }, class: :thumb) do %>
                <% if file.image? %>
                  <%= image_tag file.thumb_url, alt: file.name %>
                <% else %>
                  <span class="ext icon-<%= file.extname %>"><%= file.extname %></span>
                <% end %>
              <% end %>

              <div class="name">
                <label>
                  <%= file.name %><br/>
                  <%= file.filename %>
                </label><br/>
              </div>
              <% page = Cms::Page.in(file_ids: file.id).first %>
              <% next if page.nil? %>
              <%= link_to({ action: :show, id: file.id }) do %>
              <span class="name">
                <label>
                  <%= page.name %><br/>
                  <%= page.filename %>
                </label><br/>
              </span>
              <% end %>
            </div>
          <% end %>
        </div>
      </dd>
    </dl>

    <%= paginate @items %>
  </div>
</div>
